<databaseChangeLog
  xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
    http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-2.0.xsd">

  <changeSet id="2023-06-14--14-00-create-queue-item-table" author="EricValuk">
    <createTable  tableName="dataImportQueueItems">
      <column name="id" type="uuid">
        <constraints primaryKey="true" nullable="false"/>
      </column>
      <column name="jobExecutionId" type="uuid">
      </column>
	  <column name="uploadDefinitionId" type="uuid">
      </column>
	  <column name="size" type="bigint">
      </column>
	  <column name="originalSize" type="bigint">
      </column>
	  <column name="filePath" type="text">
      </column>
	  <column name="timestamp" type="datetime">
      </column>
    </createTable>
  </changeSet>

</databaseChangeLog>